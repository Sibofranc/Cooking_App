<UserControl x:Class="Cooking_App.View.UserControls.Toolbar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Cooking_App.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Height="75" VerticalAlignment="Top" Background="#FFC9C8C8">
        <Rectangle Margin="605,10,135,10" RadiusY="37.5" RadiusX="37.5">
            <Rectangle.Fill>
                <ImageBrush ImageSource="{Binding URL}"/>
            </Rectangle.Fill>
        </Rectangle>
    </Grid>
</UserControl>
